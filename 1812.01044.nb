  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{
    "n", ",", "A", ",", "Adg", ",", "Id", ",", "Idtil", ",", "HhoEnergy", ",",
      "HhoPos", ",", "HhoEnergyE", ",", "X", ",", "P", ",", "Xpos", ",", 
     "Ppos", ",", "F", ",", "Fdg"}], "]"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"number", " ", "of", " ", "qubits"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "=", "5"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Id", "=", 
     RowBox[{"ParallelTable", "[", 
      RowBox[{
       TemplateBox[{
         RowBox[{"i", "-", "j"}]},
        "KroneckerDeltaSeq"], " ", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"2", "^", "n"}], ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", 
         RowBox[{"2", "^", "n"}], ",", "1"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"equation", " ", "5"}], ":", " ", 
     RowBox[{"define", " ", "A"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"A", "=", 
     RowBox[{"ParallelTable", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Sqrt", "[", "j", "]"}], 
        TemplateBox[{
          RowBox[{"i", "-", "1", "-", "j"}]},
         "KroneckerDeltaSeq"]}], " ", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"2", "^", "n"}], ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", 
         RowBox[{"2", "^", "n"}], ",", "1"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"define", " ", "A", " ", "dagger"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Adg", "=", 
     RowBox[{"ConjugateTranspose", "[", "A", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"equation", " ", "17"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Adg", ".", "A"}], "-", 
     RowBox[{"A", ".", "Adg"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "using", " ", "this", " ", "form", " ", "would", " ", "give", " ", "the", 
     " ", "exact", " ", "result", " ", "for", " ", "the", " ", 
     "eigenenergies"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"A", ".", "Adg"}], "+", 
     RowBox[{"Id", "/", "2"}]}], " ", ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"equation", " ", "14"}], ":", " ", 
     RowBox[{"we", " ", "use", " ", "this", " ", "instead"}]}], "  ", "*)"}], 
   " ", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"HhoEnergy", "=", 
     RowBox[{"0.5", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"A", ".", "Adg"}], "+", 
        RowBox[{"Adg", ".", "A"}]}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
   
   RowBox[{"(*", " ", 
    RowBox[{
    "eigenvalues", " ", "will", " ", "appear", " ", "in", " ", "reverse", " ",
      "order"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
