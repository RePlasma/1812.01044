   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"equation", " ", "6"}], ":", " ", 
     RowBox[{"position", " ", "momentum", " ", "operators"}]}], "  ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"X", "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"\[Sqrt]", "2"}]], 
       RowBox[{"(", 
        RowBox[{"Adg", "+", "A"}], ")"}]}]}], ";", 
     RowBox[{"P", "=", 
      RowBox[{
       FractionBox["I", 
        RowBox[{"\[Sqrt]", "2"}]], 
       RowBox[{"(", 
        RowBox[{"Adg", "-", "A"}], ")"}]}]}], ";"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Xpos", "=", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
