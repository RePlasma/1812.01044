Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "n", ",", "Nv", ",", "\[Alpha]", ",", "FN", ",", "FNinv", ",", "H", ",", 
    "x", ",", "\[Sigma]", ",", "GS", ",", "E0", ",", "E1", ",", "E2", ",", 
    "psi", ",", "xt", ",", "Hd", ",", "th0", ",", "th1", ",", "th2", ",", 
    "dx", ",", "Id", ",", "V", ",", "K"}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "5"}], ";", 
  RowBox[{"(*", 
   RowBox[{"number", " ", "of", " ", "qubits"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"Nv", "=", 
   RowBox[{"2", "^", "n"}]}], ";", 
  RowBox[{"(*", " ", 
   RowBox[{"grid", " ", "size"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"\[Alpha]", "=", 
   RowBox[{"1", "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"0.5", "-", 
        RowBox[{"Nv", "/", "2"}]}], ")"}], "^", "2"}], ")"}]}]}], ";", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"normalization", " ", "of", " ", "x"}], "-", "domain"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"x", "=", 
   RowBox[{"3", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Sqrt", "[", "\[Alpha]", "]"}], 
       RowBox[{"(", 
        RowBox[{"i", "+", "0.5", "-", 
         RowBox[{"Nv", "/", "2"}]}], ")"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "0", ",", 
        RowBox[{"Nv", "-", "1"}], ",", "1"}], "}"}]}], "]"}]}]}], ";", 
  RowBox[{"(*", 
   RowBox[{"x", "-", "domain"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"dx", " ", "=", " ", 
   RowBox[{
    RowBox[{"x", "[", 
     RowBox[{"[", "2", "]"}], "]"}], "-", 
    RowBox[{"x", "[", 
     RowBox[{"[", "1", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Id", "=", 
    RowBox[{"IdentityMatrix", "[", "Nv", "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"kinetic", " ", "energy"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"K", "=", 
   RowBox[{"Id", "*", "0"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "<=", "Nv"}], ",", 
    RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", ">", "1"}], ",", 
       RowBox[{
        RowBox[{"K", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", 
           RowBox[{"i", "-", "1"}]}], "]"}], "]"}], "=", "1"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"K", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "i"}], "]"}], "]"}], "=", 
      RowBox[{"-", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", "<", "Nv"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"K", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", 
            RowBox[{"i", "+", "1"}]}], "]"}], "]"}], "=", "1"}], ";"}]}], 
      "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"K", "=", 
    RowBox[{
     RowBox[{"-", "0.5"}], 
     RowBox[{"K", "/", 
      RowBox[{"dx", "^", "2"}]}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"V", "=", 
    RowBox[{"0.5", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"x", "^", "2"}], "]"}]}]}], ";", 
   RowBox[{"(*", " ", 
    RowBox[{"quadratic", " ", "potential", " ", "energy"}], " ", "*)"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"H", "=", 
    RowBox[{"V", "+", "K"}]}], ";", 
   RowBox[{"(*", " ", "hamiltonian", " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"H", "//", "MatrixPlot"}]}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"plot", " ", "matrix"}], " ", "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{"eval", ",", "evec"}], "}"}], "=", 
    RowBox[{"Transpose", "[", 
     RowBox[{"Sort", "[", 
      RowBox[{"Transpose", "[", 
       RowBox[{"Eigensystem", "[", "H", "]"}], "]"}], "]"}], "]"}]}], ";", 
   RowBox[{"(*", 
    RowBox[{"get", " ", "sorted", " ", "eigensystem"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"ListPlot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"eval", ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"i", "+", "1"}], ",", 
           RowBox[{"i", "+", "0.5"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", 
           RowBox[{"Nv", "-", "1"}]}], "}"}]}], "]"}]}], "}"}], ",", 
     RowBox[{"Joined", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"False", ",", " ", "True"}], "}"}]}], ",", 
     RowBox[{"AxesLabel", "->", 
      RowBox[{"{", 
       RowBox[{"\"\<n\>\"", ",", "\"\<Energy\>\""}], "}"}]}]}], "]"}]}], 
  RowBox[{"(*", " ", "eigenvalues", " ", "*)"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"save", " ", "first", " ", "3", " ", "eigenstates"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"E0", ",", "E1", ",", "E2"}], "}"}], "=", 
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"evec", "[", 
      RowBox[{"[", 
       RowBox[{"1", ";;", "3"}], "]"}], "]"}], "]"}], "^", "2"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"nrm", "=", 
    RowBox[{"Max", "[", "E0", "]"}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"theoretical", " ", "analytical", " ", "eigenstates"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"psi", "[", 
    RowBox[{"n_", ",", "x_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"1", "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{"2", "^", "n"}], " ", 
         RowBox[{"Factorial", "[", "n", "]"}]}], "]"}]}], " ", 
      RowBox[{"Pi", "^", 
       RowBox[{"(", 
        RowBox[{"1", "/", "4"}], ")"}]}]}], ")"}], " ", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"x", "^", "2"}]}], "/", "2"}], "]"}], " ", 
    RowBox[{"HermiteH", "[", 
     RowBox[{"n", ",", "x"}], "]"}]}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", "normalizing", " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"th0", " ", "=", " ", 
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"psi", "[", 
      RowBox[{"0", ",", "x"}], "]"}], "]"}], "^", "2"}]}], ";", 
  RowBox[{"th0", "=", 
   RowBox[{
    RowBox[{"th0", "/", 
     RowBox[{"Max", "[", "th0", "]"}]}], "*", 
    RowBox[{"Max", "[", "E0", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"th1", " ", "=", " ", 
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"psi", "[", 
      RowBox[{"1", ",", "x"}], "]"}], "]"}], "^", "2"}]}], ";", 
  RowBox[{"th1", "=", 
   RowBox[{
    RowBox[{"th1", "/", 
     RowBox[{"Max", "[", "th1", "]"}]}], "*", 
    RowBox[{"Max", "[", "E1", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"th2", " ", "=", " ", 
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{"psi", "[", 
       RowBox[{"2", ",", "x"}], "]"}], "]"}], "^", "2"}]}], ";", 
   RowBox[{"th2", "=", 
    RowBox[{
     RowBox[{"th2", "/", 
      RowBox[{"Max", "[", "th2", "]"}]}], "*", 
     RowBox[{"Max", "[", "E2", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", "plot", " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
